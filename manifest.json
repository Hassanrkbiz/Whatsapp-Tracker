{
  "name": "Whatsapp Tracker",
  "version": "1.0",
  "description": "Track when a user gets online and offline on whatsapp",
  "permissions": ["activeTab","storage","notifications","https://web.whatsapp.com/*","https://notify.run/*"],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
         "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/tracker16.png",
        "32": "images/tracker32.png",
        "48": "images/tracker48.png",
        "128": "images/tracker128.png"
      }
    },
        "icons": {
        "16": "images/tracker16.png",
        "32": "images/tracker32.png",
        "48": "images/tracker48.png",
        "128": "images/tracker128.png"
    },
  "manifest_version": 2
}